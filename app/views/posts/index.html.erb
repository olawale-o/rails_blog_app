<div class="profile__container">
  <%= render partial: "users/user_card", locals: { user: { name: "user", posts: 1} } %>
  <ul class="post__comment">
    <% (1..3).each do |n| %>
      <%= render partial: "post_with_comment_card", locals: { no: n } %>
    <% end %>
  </ul>
  <button class="btn">Pagination</button>
</div>
